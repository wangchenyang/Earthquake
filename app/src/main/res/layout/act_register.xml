<?xml version="1.0" encoding="utf-8"?>
<com.zhy.android.percent.support.PercentLinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#FFFFFF"
    android:orientation="vertical">

    <com.zhy.android.percent.support.PercentLinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        app:layout_heightPercent="11%w">

        <ImageView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="center_vertical"
            android:background="@drawable/ic_arrow_back_black_24dp"
            android:clickable="true"
            android:onClick="finish"
            app:layout_heightPercent="90%h"
            app:layout_marginLeftPercent="10%h"
            app:layout_widthPercent="90%h" />

        <TextView
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:gravity="center"
            android:text="用户注册"
            android:textColor="#333333"
            android:textSize="16sp"
            app:layout_marginRightPercent="100%h" />
    </com.zhy.android.percent.support.PercentLinearLayout>

    <View
        android:layout_width="match_parent"
        android:layout_height="1dp"
        android:background="@color/main_divider" />

    <ScrollView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="@drawable/bg_71caf8_0px"
        android:padding="2dp"
        android:scrollbarThumbVertical="@android:color/transparent"
        app:layout_marginLeftPercent="7.8%w"
        app:layout_marginRightPercent="7.8%w"
        app:layout_marginTopPercent="7.8%w">

        <com.zhy.android.percent.support.PercentLinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="vertical">

            <com.zhy.android.percent.support.PercentLinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                app:layout_heightPercent="11%w"
                app:layout_marginLeftPercent="10%w"
                app:layout_marginRightPercent="10%w"
                app:layout_marginTopPercent="8%w">

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    android:gravity="center"
                    android:text="    用户名："
                    android:textColor="#333333"
                    android:textSize="14sp"
                    app:layout_marginRightPercent="3%w" />

                <EditText
                    android:id="@+id/et_user_name"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:background="@drawable/bg_edittext_0px"
                    android:paddingLeft="10dp"
                    android:paddingRight="10dp"
                    android:singleLine="true"
                    android:textColor="#333333"
                    android:textSize="14sp" />
            </com.zhy.android.percent.support.PercentLinearLayout>

            <com.zhy.android.percent.support.PercentLinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                app:layout_heightPercent="11%w"
                app:layout_marginLeftPercent="10%w"
                app:layout_marginRightPercent="10%w"
                app:layout_marginTopPercent="5%w">

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    android:gravity="center"
                    android:text="        密码："
                    android:textColor="#333333"
                    android:textSize="14sp"
                    app:layout_marginRightPercent="3%w" />

                <EditText
                    android:id="@+id/et_password"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:background="@drawable/bg_edittext_0px"
                    android:paddingLeft="10dp"
                    android:paddingRight="10dp"
                    android:singleLine="true"
                    android:textColor="#333333"
                    android:textSize="14sp" />
            </com.zhy.android.percent.support.PercentLinearLayout>

            <com.zhy.android.percent.support.PercentLinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                app:layout_heightPercent="11%w"
                app:layout_marginLeftPercent="10%w"
                app:layout_marginRightPercent="10%w"
                app:layout_marginTopPercent="5%w">

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    android:gravity="center"
                    android:text="真实姓名："
                    android:textColor="#333333"
                    android:textSize="14sp"
                    app:layout_marginRightPercent="3%w" />

                <EditText
                    android:id="@+id/et_real_name"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:background="@drawable/bg_edittext_0px"
                    android:paddingLeft="10dp"
                    android:paddingRight="10dp"
                    android:singleLine="true"
                    android:textColor="#333333"
                    android:textSize="14sp" />
            </com.zhy.android.percent.support.PercentLinearLayout>

            <com.zhy.android.percent.support.PercentLinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                app:layout_heightPercent="11%w"
                app:layout_marginLeftPercent="10%w"
                app:layout_marginRightPercent="10%w"
                app:layout_marginTopPercent="5%w">

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    android:gravity="center"
                    android:text="        电话："
                    android:textColor="#333333"
                    android:textSize="14sp"
                    app:layout_marginRightPercent="3%w" />

                <EditText
                    android:id="@+id/et_tel"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:background="@drawable/bg_edittext_0px"
                    android:paddingLeft="10dp"
                    android:paddingRight="10dp"
                    android:singleLine="true"
                    android:textColor="#333333"
                    android:textSize="14sp" />
            </com.zhy.android.percent.support.PercentLinearLayout>

            <com.zhy.android.percent.support.PercentLinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                app:layout_heightPercent="11%w"
                app:layout_marginLeftPercent="10%w"
                app:layout_marginRightPercent="10%w"
                app:layout_marginTopPercent="5%w">

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    android:gravity="center"
                    android:text="所属街道："
                    android:textColor="#333333"
                    android:textSize="14sp"
                    app:layout_marginRightPercent="3%w" />

                <TextView
                    android:id="@+id/tv_street"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:background="@drawable/bg_edittext_0px"
                    android:clickable="true"
                    android:gravity="center_vertical"
                    android:onClick="chooseStreet"
                    android:paddingLeft="10dp"
                    android:paddingRight="10dp"
                    android:singleLine="true"
                    android:textColor="#333333"
                    android:textSize="14sp" />
            </com.zhy.android.percent.support.PercentLinearLayout>

            <com.zhy.android.percent.support.PercentLinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                app:layout_heightPercent="11%w"
                app:layout_marginLeftPercent="10%w"
                app:layout_marginRightPercent="10%w"
                app:layout_marginTopPercent="5%w">

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    android:gravity="center"
                    android:text="所属社区："
                    android:textColor="#333333"
                    android:textSize="14sp"
                    app:layout_marginRightPercent="3%w" />

                <TextView
                    android:id="@+id/tv_community"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:background="@drawable/bg_edittext_0px"
                    android:clickable="true"
                    android:gravity="center_vertical"
                    android:onClick="chooseCommunity"
                    android:paddingLeft="10dp"
                    android:paddingRight="10dp"
                    android:singleLine="true"
                    android:textColor="#333333"
                    android:textSize="14sp" />
            </com.zhy.android.percent.support.PercentLinearLayout>

            <Button
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:background="@drawable/bg_press_blue_6px"
                android:clickable="true"
                android:onClick="submit"
                android:text="提交"
                android:textColor="@android:color/white"
                android:textSize="14sp"
                app:layout_heightPercent="11%w"
                app:layout_marginBottomPercent="7%w"
                app:layout_marginLeftPercent="25%w"
                app:layout_marginRightPercent="25%w"
                app:layout_marginTopPercent="7%w" />

        </com.zhy.android.percent.support.PercentLinearLayout>
    </ScrollView>
</com.zhy.android.percent.support.PercentLinearLayout>