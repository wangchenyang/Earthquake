<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_pressed="true">
        <shape>
            <corners android:radius="6px" />
            <stroke android:width="0dp" android:color="#FF198BD4" />
            <solid android:color="#FF0081C1"></solid>
        </shape>
    </item>
    <item>
        <shape>
            <corners android:radius="6px" />
            <stroke android:width="0dp" android:color="#FFD7D7D7" />
            <solid android:color="#FF2795DC"></solid>
        </shape>
    </item>
</selector>